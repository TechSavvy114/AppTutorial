<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#020202"
    android:layout_height="match_parent"
    tools:context=".Jsinterview">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:textSize="20dp"
                android:gravity="center"
                android:padding="10dp"
                android:background="#FFEB3B"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="JavaScript Interview Questions"/>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_marginTop="5dp"
                android:background="#040305"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="what do you understand by javascript?"/>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="How js is different from java?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="explain various data type of js?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="what are advantage of using javascript?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="explain the use of debugger in js?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="what are different type of error in js?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="explain self invoking function"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="difference between attributes and property?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="what are the difference between let and var?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="what is closures in js?"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="explain negative_infinity in js"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="how you empty an array in js"/>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="explain simple js dom"/>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:background="#040305"
                android:layout_marginTop="4dp"
                android:layout_height="wrap_content"
                app:cardCornerRadius="25dp">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:text="explain strict mode in js"/>
            </androidx.cardview.widget.CardView>

        </LinearLayout>

    </RelativeLayout>
</ScrollView>